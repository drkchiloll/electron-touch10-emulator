{
  "name": "cisco-touch10-emulator",
  "version": "1.0.0",
  "description": "An Emulator for the Cisco Touch 10 Interface",
  "main": "./dist/main.bundle.js",
  "scripts": {
    "build:dev": "node_modules/.bin/webpack --watch --progress --profile",
    "build:dev:win": "SET \"NODE_ENV=development\" && webpack --watch --progress --profile",
    "build:dist": "NODE_ENV=production node_modules/.bin/webpack --progress --colors",
    "build:win:dist": "SET \"NODE_ENV=production\" && webpack --define process.env.NODE_ENV=\"production\" --progress --colors",
    "start": "electron .",
    "package:macos": "node_modules/.bin/electron-packager ./ Touch10_Emulator --platform=darwin --arch=x64 --prune=true --ignore=src/app --ignore=node_modules/@types --ignore=node_modules/electron* --app-version=1.0.0 --icon ./resources/touch10_panel.icns",
    "package:win64": "node_modules/.bin/electron-packager ./ Touch10_Emulator --platform=win32 --arch=x64 --prune=true --ignore=src/app --ignore=node_modules/@types --ignore=node_modules/electron* --app-version=1.0.0 --icon ./resources/touch10_panel.ico",
    "clean": "cd dist && rm app.bundle.js app.bundle.js.map main.bundle.js main.bundle.js.map && cd .."
  },
  "dependencies": {},
  "devDependencies": {
    "@types/material-ui": "^0.21.1",
    "@types/node": "^7.0.55",
    "@types/react": "^15.0.14",
    "@types/react-dom": "^0.14.23",
    "awesome-typescript-loader": "^3.5.0",
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "bufferutil": "^3.0.3",
    "css-loader": "^0.26.1",
    "electron": "^1.8.2",
    "electron-packager": "^11.1.0",
    "electron-rebuild": "^1.7.3",
    "file-loader": "^0.10.1",
    "json-loader": "^0.5.4",
    "jsxapi": "^4.1.2",
    "material-ui": "^0.20.0",
    "moment": "^2.21.0",
    "moment-timezone": "^0.5.14",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "style-loader": "^0.13.1",
    "typescript": "^2.7.2",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "url-loader": "^0.5.6",
    "utf-8-validate": "^4.0.0",
    "webpack": "^2.2.1",
    "webpack-merge": "^3.0.0"
  }
}
