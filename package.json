{
  "name": "cisco-video-endpoint-emulator",
  "version": "1.0.3",
  "description": "An Emulator for Cisco Video Endpoint Codecs",
  "main": "./dist/main.bundle.js",
  "scripts": {
    "build:dev": "node_modules/.bin/webpack --watch",
    "build:dev:win": "SET \"NODE_ENV=development\" && webpack --watch --progress --profile",
    "build:dist": "NODE_ENV=production node_modules/.bin/webpack --progress --colors",
    "build:win:dist": "SET \"NODE_ENV=production\" && webpack --define process.env.NODE_ENV=\"production\" --progress --colors",
    "start": "electron .",
    "package:macos": "node_modules/.bin/electron-packager ./ VideoCodec-Emulator --platform=darwin --arch=x64 --prune=true --ignore=src/app --ignore=node_modules/@types --ignore=node_modules/electron* --app-version=1.0.4 --icon ./resources/touch10_panel.icns",
    "package:win64": "node_modules/.bin/electron-packager ./ VideoCodec_Emulator --platform=win32 --arch=x64 --prune=true --ignore=src/app --ignore=node_modules/@types --ignore=node_modules/electron* --app-version=1.0.3 --icon ./resources/touch10_panel.ico",
    "clean": "cd dist && rm app.bundle.js app.bundle.js.map main.bundle.js main.bundle.js.map && cd .."
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^7.2.6",
    "@types/material-ui": "^0.21.1",
    "@types/node": "^7.0.55",
    "@types/react": "^15.0.14",
    "@types/react-datepicker": "^1.1.2",
    "@types/react-dom": "^0.14.23",
    "@types/uniqid": "^4.1.2",
    "awesome-typescript-loader": "^3.5.0",
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "bufferutil": "^3.0.3",
    "ciscospark": "^1.30.2",
    "createref": "^0.5.0",
    "css-loader": "^0.26.1",
    "easyxml": "^2.0.1",
    "electron": "^1.8.4",
    "electron-packager": "^11.1.0",
    "electron-rebuild": "^1.7.3",
    "file-loader": "^0.10.1",
    "json-loader": "^0.5.4",
    "jsonwebtoken": "^8.2.1",
    "jsxapi": "^4.1.2",
    "material-ui": "^0.20.0",
    "material-ui-slider-label": "git+https://github.com/tariqwest/material-ui-slider-label.git",
    "moment": "^2.21.0",
    "moment-timezone": "^0.5.14",
    "nipplejs": "^0.6.8",
    "react": "^16.2.0",
    "react-chips": "^0.6.4",
    "react-datepicker": "^1.4.1",
    "react-dom": "^16.2.0",
    "react-flexbox-grid": "^2.0.0",
    "react-joystick": "^1.0.3",
    "react-modal-resizable-draggable": "^0.1.0",
    "react-transition-group": "^2.3.1",
    "style-loader": "^0.13.1",
    "typescript": "^2.7.2",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "uniqid": "^4.1.1",
    "url-loader": "^0.5.6",
    "utf-8-validate": "^4.0.0",
    "webpack": "^2.2.1",
    "webpack-merge": "^3.0.0"
  },
  "dependencies": {}
}
